export MOUNTPOINT=/tmp/snapfaas
export MEMSNAPSHOTDIR=$MOUNTPOINT/images/snapshot
export MEMROOTFSDIR=$MOUNTPOINT/images/rootfs
export MEMAPPFSDIR=$MOUNTPOINT/images/appfs
export SSDSNAPSHOTDIR=/ssd/images/snapshot
export SSDROOTFSDIR=/ssd/images/rootfs
export SSDAPPFSDIR=/ssd/images/appfs
export SSDEXECSNAPSHOTDIR=/ssd/images/exec-snapshot
export KERNEL=$MOUNTPOINT/images/vmlinux-4.20.0

export MEMBINDIR=$MOUNTPOINT/bin/release

export SSDPREEXECSNAPSHOTDIR=/ssd/images/preexec-snapshot
export MEMPREEXECSNAPSHOTDIR=/tmp/snapfaas/images/preexec-snapshot
export SSDJITLESSSNAPSHOTDIR=/ssd/images/jitless-snapshot
export MEMJITLESSSNAPSHOTDIR=/tmp/snapfaas/images/jitless-snapshot
